;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "",
"class": "GitAPI",
"language": "NativeScript",
"path": "res://addons/godot-git-plugin/git_api.gdns"
} ]
_global_script_class_icons={
"GitAPI": ""
}

[application]

config/name="MRalV2"
run/main_scene="res://Scenes/MainMenu.tscn"
boot_splash/image="res://Images/introImage.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
boot_splash/bg_color=Color( 0.121569, 0.25098, 0.207843, 1 )
config/icon="res://icon.png"
config/windows_native_icon="res://Images/Export/Windows/icon.ico"
config/quit_on_go_back=false

[audio]

default_bus_layout="res://default.tres"

[display]

window/size/width=1920
window/size/height=1080
window/size/fullscreen=true
window/size/test_width=1280
window/size/test_height=720
window/dpi/allow_hidpi=true
window/stretch/mode="viewport"
window/stretch/aspect="expand"
window/size/fullscreen.editor=false

[editor]

version_control_autoload_on_startup=true
version_control_plugin_name="GitAPI"

[gdnative]

singletons=[ "res://addons/godot-git-plugin/git_api.gdnlib" ]

